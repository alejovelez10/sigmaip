<p id="notice"><%= notice %></p>

<h1>Solicitudes de Trabajo</h1>

<table class="table">
  <thead>
    <tr>
      <th>Fecha</th>
      <th>Nombre</th>
      <th>Email</th>
      <th>Telefono</th>
      <th>Nivel de Estudio</th>
      <th>Adjunto</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jobs.each do |job| %>
      <tr>
      <td><%= get_date(job.created_at) %></td>
        <td><%= job.name %></td>
        <td><%= job.phone %></td>
        <td><%= job.email %></td>
        <td><%= job.level %></td>
        <td><%= link_to "Adjunto" , job.file_url if job.file_url %></td>
        <td><%= job.description %></td>
        
        <td><%= link_to 'Eliminar', job, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job', new_job_path %>
