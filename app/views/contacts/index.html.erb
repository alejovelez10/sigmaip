<p id="notice"><%= notice %></p>

<h1>Contactos</h1>

<table class="table">
  <thead>
    <tr>
     <th>Fecha</th>
      <th>Nombre</th>
      <th>Asunto</th>
      <th>Email</th>
      <th>Mensaje</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contacts.each do |contact| %>
      <tr>
      <td><%= get_date(contact.created_at) %></td>
        <td><%= contact.name %></td>
        <td><%= contact.subject %></td>
        <td><%= contact.email %></td>
        <td><%= contact.message %></td>
        <td><%= link_to 'Eliminar', contact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


